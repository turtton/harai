# Cloudflare D1 データベース設定
CLOUDFLARE_ACCOUNT_ID=your_account_id_here
CLOUDFLARE_DATABASE_ID=your_database_id_here
CLOUDFLARE_D1_TOKEN=your_d1_token_here

# 実際の値の例:
# CLOUDFLARE_ACCOUNT_ID=db1d9ab27hd9dfbdfd8dbf9ss555c9h
# CLOUDFLARE_DATABASE_ID=5a7sbn2a-4scb-4dd3-7dec-d1b535a96d59
# CLOUDFLARE_D1_TOKEN=your_api_token_with_d1_edit_permissions

# 開発環境用の設定
NODE_ENV=development

# API 認証トークン (将来の実装用)
# API_TOKEN=your_secure_api_token_here